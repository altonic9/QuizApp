<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.QuizMenuController">
   <children>
      <Label alignment="CENTER" layoutX="229.0" layoutY="74.0" text="Quiz Menu" textAlignment="CENTER" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <font>
            <Font size="50.0" />
         </font>
      </Label>
      <Button fx:id="startWithNewProfileBTN" layoutX="234.0" layoutY="69.0" mnemonicParsing="false" onAction="#openStartWithNewProfile" prefHeight="130.0" prefWidth="150.0" text="Start with new Profile" />
      <Button fx:id="loadProfileBTN" layoutX="234.0" layoutY="224.0" mnemonicParsing="false" onAction="#openLoadProfile" prefHeight="130.0" prefWidth="150.0" text="Load Profile" />
      <Button fx:id="returnToMainMenu" layoutX="391.0" layoutY="359.0" mnemonicParsing="false" onAction="#openStartWindow" prefHeight="27.0" prefWidth="141.0" text="Return to Main Menu" AnchorPane.bottomAnchor="0.0" />
